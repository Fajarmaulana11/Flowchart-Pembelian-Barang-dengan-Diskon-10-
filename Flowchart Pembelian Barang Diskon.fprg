                                <output expression="&quot;Program Studi =&quot;" newline="True"/>
                                <output expression="prodi" newline="True"/>
                                <output expression="&quot;Pembelian Barang&quot;" newline="True"/>
                                <output expression="&quot;Harga Barang 1 =&quot;" newline="True"/>
                                <output expression="b1" newline="True"/>
                                <output expression="&quot;Harga Barang 2 =&quot;" newline="True"/>
                                <output expression="b2" newline="True"/>
                                <output expression="&quot;Harga Barang 3 =&quot;" newline="True"/>
                                <output expression="b3" newline="True"/>
                                <output expression="&quot;Jumlah Belanja =&quot;" newline="True"/>
                                <output expression="jb" newline="True"/>
                                <output expression="&quot;Diskon 10% =&quot;" newline="True"/>
                                <output expression="d" newline="True"/>
                                <output expression="&quot;Total Pembayaran =&quot;" newline="True"/>
                                <output expression="bayar" newline="True"/>
                            </then>
                            <else>
                                <assign variable="d" expression="0"/>
                                <assign variable="bayar" expression="jb"/>
                                <output expression="&quot;Struk Pembelian Barang&quot;" newline="True"/>
                                <output expression="&quot;Nama =&quot;" newline="True"/>
                                <output expression="nama" newline="True"/>
