                                <output expression="&quot;Struk Pembelian Barang&quot;" newline="True"/>
                                <output expression="&quot;Nama =&quot;" newline="True"/>
                                <output expression="nama" newline="True"/>
                                <output expression="&quot;Nomor Antrian =&quot;" newline="True"/>
